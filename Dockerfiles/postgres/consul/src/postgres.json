{
  "service": {
    "name": "db",
    "tags": ["{{env "PROJECT"}}", "{{env "MODE"}}"],
    "port": 5432,
    "check": {
      "#": "script returns 1 if Ok but consul waits 0, so use '!'",
      "script": "! gosu postgres check_postgres.pl --action connection >/dev/null",
      "interval": "60s"
    }
  }
}